{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "env": {
    "vue/setup-compiler-macros": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "import/order": 0
  }
}
